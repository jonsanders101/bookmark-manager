<%= p "is links view rendered?" %>


<h1> Here be links. </h1>
<h2> Welcome, <%=@user_email%>!</h2>

<ul id='links'>
  <% @links.each do |link| %>
    <li>
      Title: <%= link.title %><br>
      URL:   <%= link.url   %><br>
      Tags:   <%= link.tags.map(&:name).join(", ") %> <br>
    </li>
  <% end %>
</ul>
